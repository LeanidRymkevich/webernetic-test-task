{
  "compilerOptions": {
    "target": "es2016",
    "module": "ES2022",
    "moduleResolution": "node",
    "esModuleInterop": true,                             
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "@src/*": ["./src/*"],
    }
  }
}
